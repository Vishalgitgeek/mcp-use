{
  "mcpServers": {
    "filesystem": {
      "command": "cmd",
      "args": [
        "/c", "npx", "-y", "@modelcontextprotocol/server-filesystem", 
        "${FILESYSTEM_PATH}"
      ]
    },
    "supabase": {
      "command": "cmd",
      "args": [
        "/c", "npx", "-y", "@supabase/mcp-server-supabase", 
        "--access-token", "${SUPABASE_KEY}"
      ]
    }
  }
}


